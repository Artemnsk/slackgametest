{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-eslint-rules"
    ],
    "jsRules": {
      "indent": [true, "spaces", 2]
    },
    "rules": {
      "indent": [true, "spaces", 2],
      "max-line-length": [false],
      "interface-over-type-literal": false,
      "no-shadowed-variable": false,
      "object-curly-spacing": true
    },
    "rulesDirectory": []
}